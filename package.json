{
  "name": "vendor-frontend-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run vendor-frontend-ui",
    "build": "ng build",
    "test": "jest --runInBand --detectOpenHandles  --config ./jest.config.js",
    "test:coverage": "jest --coverage --config ./jest.config.js",
    "test:clear": "jest --clearCache",
    "sonar": "sonar-scanner",
    "lint": "eslint --color --fix -c .eslintrc.js --ext .ts ./projects/vendor-frontend-ui/src/app/features/payments/bank-status",
    "e2e": "ng e2e",
    "i18n": "npm run build:i18n:vendor-frontend-ui",
    "vendor-frontend-ui": "ng serve --project=vendor-frontend-ui --aot=true",
    "vendor-frontend-ui:turbo": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng serve --project=vendor-frontend-ui --aot=true",
    "sandbox": "ng serve --project=sandbox --aot=true --port=4300",
    "build:vendor-frontend-ui": "ng build --project=vendor-frontend-ui",
    "build:vendor-frontend-ui:turbo": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build --project=vendor-frontend-ui --base-href=/vendor-frontend-ui/ --prod=true",
    "build:vendor-frontend-ui:prod": "ng build --project=vendor-frontend-ui --prod=true",
    "build:vendor-frontend-ui:turbo-prod": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build --project=vendor-frontend-ui --prod=true",
    "build:i18n:base": "npm run build:i18n:base-de && npm run build:i18n:base-en",
    "build:i18n:base-de": "json-merger assets/i18n/base-de.json > dist/base-de.json",
    "build:i18n:base-en": "json-merger assets/i18n/base-en.json > dist/base-en.json",
    "build:i18n:apps": "npm run build:i18n:apps-de && npm run build:i18n:apps-en",
    "build:i18n:apps-de": "json-merger assets/i18n/apps-de.json > dist/apps-de.json",
    "build:i18n:apps-en": "json-merger assets/i18n/apps-en.json > dist/apps-en.json",
    "build:i18n:libs": "npm run build:i18n:libs-de && npm run build:i18n:libs-en",
    "build:i18n:libs-de": "json-merger assets/i18n/libs-de.json > dist/libs-de.json",
    "build:i18n:libs-en": "json-merger assets/i18n/libs-en.json > dist/libs-en.json",
    "build:i18n": "npm run build:i18n:de && npm run build:i18n:en",
    "build:i18n:de": "json-merger assets/i18n/de.json > dist/de.json",
    "build:i18n:en": "json-merger assets/i18n/en.json > dist/en.json",
    "build:i18n:vendor-frontend-ui": "npx mkdirp projects/vendor-frontend-ui/src/assets/i18n/ && npm run build:i18n:vendor-frontend-ui:de && npm run build:i18n:vendor-frontend-ui:en",
    "build:i18n:vendor-frontend-ui:de": "json-merger assets/i18n/de.json > projects/vendor-frontend-ui/src/assets/i18n/de.json",
    "build:i18n:vendor-frontend-ui:en": "json-merger assets/i18n/en.json > projects/vendor-frontend-ui/src/assets/i18n/en.json",
    "build:i18n:sandbox": "npx mkdirp projects/sandbox/src/assets/i18n/ && npm run build:i18n:sandbox:de && npm run build:i18n:sandbox:en",
    "build:i18n:sandbox:de": "json-merger assets/i18n/de.json > projects/sandbox/src/assets/i18n/de.json",
    "build:i18n:sandbox:en": "json-merger assets/i18n/en.json > projects/sandbox/src/assets/i18n/en.json",
    "test:watch": "jest --watch",
    "compodoc": "npx compodoc -p tsconfig.json  --includes compodoc/ --config config/.compodocrc.json --output=documentation/payment-front-end-ui",
    "compodoc:watch_mode": "npx compodoc -p tsconfig.json  -s -w --includes compodoc/ --config config/.compodocrc.json --output=documentation/payment-front-end-ui",
    "eslint": "npx eslint projects/**/*.ts",
    "eslint:fix": "npx eslint projects/**/*.ts --fix",
    "eslint:tests": "npx eslint tests/**/*.ts",
    "eslint:tests:fix": "npx eslint tests/**/*.ts --fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.12",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~9.1.12",
    "@angular/compiler": "~9.1.12",
    "@angular/core": "~9.1.12",
    "@angular/elements": "^9.1.12",
    "@angular/forms": "~9.1.12",
    "@angular/platform-browser": "~9.1.12",
    "@angular/platform-browser-dynamic": "~9.1.12",
    "@angular/router": "~9.1.12",
    "@briebug/jest-schematic": "^2.1.1",
    "@ngx-translate/core": "~11.0.1",
    "@ngx-translate/http-loader": "~4.0.0",
    "@syncfusion/ej2-angular-buttons": "^18.2.54",
    "@syncfusion/ej2-angular-calendars": "^18.2.54",
    "@syncfusion/ej2-angular-charts": "^18.2.48",
    "@syncfusion/ej2-angular-dropdowns": "^18.2.57",
    "@syncfusion/ej2-angular-grids": "^18.2.47",
    "@syncfusion/ej2-angular-inputs": "^18.2.54",
    "@syncfusion/ej2-angular-navigations": "^18.2.54",
    "@syncfusion/ej2-angular-notifications": "18.2.44",
    "@syncfusion/ej2-angular-popups": "^18.2.54",
    "@syncfusion/ej2-angular-querybuilder": "^18.2.54",
    "@syncfusion/ej2-angular-splitbuttons": "^18.2.44",
    "@syncfusion/ej2-angular-treemap": "^18.3.47",
    "@syncfusion/ej2-base": "^18.2.54",
    "@syncfusion/ej2-layouts": "^18.2.54",
    "@syncfusion/ej2-notifications": "18.2.44",
    "@types/jsrsasign": "^8.0.5",
    "@types/uuid": "^3.4.5",
    "angular-oauth2-oidc": "^8.0.4",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.20",
    "moment": "^2.24.0",
    "net": "^1.0.2",
    "ngx-skeleton-loader": "^2.4.4",
    "ngx-webstorage-service": "^4.1.0",
    "rxjs": "^6.6.2",
    "sockjs-client": "^1.5.0",
    "sonar-scanner": "^3.1.0",
    "stompjs": "^2.3.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.12",
    "@angular-devkit/build-ng-packagr": "~0.803.19",
    "@angular/cli": "~9.1.11",
    "@angular/compiler-cli": "~9.1.12",
    "@angular/language-service": "~8.2.14",
    "@compodoc/compodoc": "^1.1.11",
    "@componizer/schematics": "0.0.5",
    "@types/jasmine": "^3.5.12",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^26.0.1",
    "@types/node": "^12.12.54",
    "@types/stompjs": "^2.3.4",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.4.0",
    "@typescript-eslint/parser": "^3.10.1",
    "barrelsby": "^2.2.0",
    "codelyzer": "^5.2.2",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^30.3.0",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-prettier": "^3.1.4",
    "gts": "^1.1.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^26.4.6",
    "jest-canvas-mock": "^2.2.0",
    "jest-preset-angular": "^8.3.1",
    "jest-summarizing-reporter": "^1.1.4",
    "jest-when": "^2.7.2",
    "json-merger": "^1.1.1",
    "lite-server": "^2.5.4",
    "ng-mocks": "^9.1.0",
    "ng-packagr": "^5.1.0",
    "ngx-translate-testing": "^4.0.0",
    "prettier": "^2.1.1",
    "prettier-eslint": "^11.0.0",
    "ts-node": "~7.0.0",
    "tsickle": "0.37.0",
    "typescript": "~3.8.3"
  }
}
